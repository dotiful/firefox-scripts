{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "chrome/userChrome",
    "custom",
    "./user.js"
  ],
  "verbose": false,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "chrome/scripts",
    "gulp/"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}
